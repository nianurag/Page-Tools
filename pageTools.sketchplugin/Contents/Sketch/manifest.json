{
  "name": "Page Tools",
  "identifier": "com.sketchapp.PageTools",
  "description": "Simple shortcuts to work with pages in Sketch App.",
  "version": "1.0.0",
  "author": "Anurag Adhikary",
  "authorEmail": "nianurag@gmail.com",
  "homepage": "https://github.com/nianurag/Page-Tools/",
  "compatibleVersion": "40",
  "bundleVersion": "1",
  "commands": [{
    "script": "main.js",
    "handler": "createPage",
    "shortcut": "cmd alt n",
    "name": "Create new Page",
    "identifier": "create-new-page"
  }, {
    "script": "main.js",
    "handler": "switchPages",
    "shortcut": "cmd alt ,",
    "name": "Switch Pages",
    "identifier": "switch-page"
  }],
  "menu": {
    "items": [
      "create-new-page",
      "switch-page"
    ]
  }
}
